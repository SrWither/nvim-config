" Themes
colorscheme vscode

" Editor Settings
set termguicolors
set guifont=Fira\ Code:h12
set number
set splitbelow

" CloseTag
let g:closetag_filenames = '*.html,*.xhtml,*.phtml,*.vue'
let g:closetag_filetypes = 'html,xhtml,phtml,vue'
